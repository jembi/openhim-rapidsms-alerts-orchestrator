<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns="http://www.mulesoft.org/schema/mule/core" xmlns:http="http://www.mulesoft.org/schema/mule/http" xmlns:doc="http://www.mulesoft.org/schema/mule/documentation" xmlns:spring="http://www.springframework.org/schema/beans" version="CE-3.4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mulesoft.org/schema/mule/http http://www.mulesoft.org/schema/mule/http/current/mule-http.xsd
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-current.xsd
http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd">
    <http:connector name="NoSessionConnector" doc:name="HTTPNoSessionConnector">
		<service-overrides sessionHandler="org.mule.session.NullSessionHandler" />
	</http:connector>
    <management:jmx-default-config xmlns:management="http://www.mulesoft.org/schema/mule/management" port="${jmx.port}" registerMx4jAdapter="true">  
        <management:credentials> 
            <spring:entry key="${jmx.username}" value="${jmx.password}"></spring:entry> 
        </management:credentials> 
    </management:jmx-default-config>
    <management:jmx-mx4j-adaptor xmlns:management="http://www.mulesoft.org/schema/mule/management" login="${jmx.username}" password="${jmx.password}"></management:jmx-mx4j-adaptor>
</mule>
